<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment>

   <extension
         id="CocoaUIProcessor"
         name="Cocoa UI Processor"
         point="org.eclipse.e4.workbench.model">
      <processor
            beforefragment="false"
            class="org.eclipse.e4.ui.workbench.renderers.swt.cocoa.CocoaUIProcessor">
      </processor>
   </extension>

   <extension
         point="org.eclipse.ui.internalTweaklets">
      <tweaklet
            definition="org.eclipse.ui.internal.tweaklets.TitlePathUpdater"
            id="org.eclipse.ui.cocoa.titlePathUpdaterTweaklet"
            implementation="org.eclipse.e4.ui.workbench.renderers.swt.cocoa.CocoaTitlePathUpdater"
            name="Cocoa Title Path Updater">
      </tweaklet>
   </extension>
</fragment>
